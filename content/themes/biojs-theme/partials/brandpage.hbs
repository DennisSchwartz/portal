<header class="biojs-green biojs-white-text">
  <div class="container">
    {{#get "posts" slug="devs-header"}}
    {{#foreach posts}}
        <div class="row">
          <div class="col s12">
            <h1 class="brand-title light">{{ title }}</h1>
          </div>
        </div>
        <div class="row pb-2 mb-0">
          <div class="col s12 m7">
                {{ content }} <br/> 
          </div>        
          <div class="col s12 m5">
            <p><a class="waves-effect waves-blue btn-large action uppercase" href="#"><i class="material-icons left">play_arrow</i>Get Started</a></p>
          </div>
        </div>
      </div>
    {{/foreach}}
  {{/get}}
</header>
<main>
  {{#has tag="brandpage-starters"}}
    <section class="container">
      {{> bene }}
    </section>
    <section>
      {{> example}}
    </section>
  {{else}}
    {{> component-examples}}
    <section class="biojs-black biojs-white-text">
      <div class="container">
        <div class="row">
          <div class="col s12">
            <h2 class="light">BioJS interoperability and scalability</h2>
            <p>njaldjnaldadljasdnladadjn </p>
          </div>
        </div>
      </div>
    </section>
  {{/has}}
    <section class="container">
      <div class="row">
        <div class="col s12">
          {{ content }}

        </div>
      </div>
      <hr />
    </section>
    <section class="parallax-container valign-wrapper">
      {{> tip-section}}
    </section>
    {{#has tag="brandpage-developers"}}
      {{> bene}}
    {{/has}}
    <section class="biojs-black biojs-white-text center">
      {{> showcase}}
    </section>
    {{#has tag="brandpage-starters"}}
    <section>
      <div class="container">
        <div class="row">
          <div class="col s12">
            <h2>Lorem ipsum lore CTA section title</h2>
            {{#get "posts" slug="starters-bottom"}}
              {{#foreach posts}}
                {{ content }}
              {{/foreach}}
            {{/get}}
            <p class="center mt-4 mb-2"><a href="http://edu.biojs.net/101/intro/" class="waves-effect waves-yellow action btn-large">GET STARTED</a></p>
          </div>
        </div>
      </div>
    </section>
    {{else}}
      {{#has tag="brandpage-developers"}}
    <section class="biojs-green">
      <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/octicons/2.0.2/octicons.min.css">
      <link rel="stylesheet" href="{{asset "css/github-activity-0.1.4.min.css"}}">
      <div class="container">
        <div class="row">
          <div class="col s12 m6">
            <h2 class="light biojs-white-text">Meet our growing community on Github</h2>
            <p class="biojs-white-text">Supercool text about the community</p>
            <p><iframe src="https://ghbtns.com/github-btn.html?user=biojs&repo=biojs&type=star&count=true&size=large" frameborder="0" scrolling="0" width="160px" height="30px"></iframe>
            <iframe src="https://ghbtns.com/github-btn.html?user=biojs&repo=biojs&type=fork&count=true&size=large" frameborder="0" scrolling="0" width="158px" height="30px"></iframe></p>
          </div>
          <div class="col s12 m6">
            <div id="feed" class="height-5x biojs-black-text"></div>
          </div>
        </div>
       </div>
     </section>
     <section class="container">
      <div class="row">
        <div class="col s12">
        
        </div>
      </div>
      <div class="row center">
        <div class="col s12">
          <h2 class="light">We would like to hear from you!</h2>
        </div>
        <div class="col s12 m6">
           <p><a href="https://twitter.com/biojslibrary" class="weak-action waves-effect waves-blue btn-large">Follow us on Twitter</a></p>
          {{! <a href="https://twitter.com/biojslibrary" class="twitter-follow-button" data-show-count="true" data-size="large">Follow @biojslibrary</a>
            <script src="https://platform.twitter.com/widgets.js"></script> }}
        </div>
        <div class="col s12 m6">
          <p><a href="#" class="action waves-effect waves-blue btn-large">Join our slack chat</a></p>
        </div>
      </div>
     </section>
      {{/has}}
    {{/has}}
    {{#has tag="brandpage-developers"}}
      <script src="https://cdnjs.cloudflare.com/ajax/libs/mustache.js/2.2.1/mustache.min.js"></script>
      <script type="text/javascript" src="{{asset "js/github-activity-0.1.4.min.js"}}"></script>
      <script  type="text/javascript" src="{{asset "js/fabric.min.js"}}"></script>
      <script defer src="{{asset "js/triangle-gallery.js"}}" type="text/javascript"></script>
    {{/has}}
</main>

